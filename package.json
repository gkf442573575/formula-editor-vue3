{
  "name": "formula-editor-repo",
  "version": "0.0.3",
  "private": true,
  "description": "codemirror formula editor for vue3",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:lib\" \"pnpm run dev:example\"",
    "dev:lib": "pnpm -C editor dev",
    "dev:example": "pnpm -C example dev",
    "build:lib": "pnpm -C editor build",
    "build:example": "pnpm run build:lib && pnpm -C example build",
    "bump": "bumpp package.json editor/package.json",
    "bump:example": "bumpp example/package.json --tag example- --commit example-"
  },
  "keywords": [
    "formula",
    "formula-vue3",
    "formula-editor",
    "codemirror-formula",
    "codemirror-formula-vue3",
    "codemirror6-formula-vue3",
    "codemirror6-js-formula"
  ],
  "author": "gkf442573575@163.com",
  "license": "MIT",
  "devDependencies": {
    "bumpp": "^9.4.1",
    "concurrently": "^8.2.2"
  }
}
